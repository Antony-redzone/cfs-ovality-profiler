    PVGraphReport.Sections.Item("Section2").Controls.Item("lblLineDesc1").Caption = DisplayMessage("Upper limit = ") & PVXScaleLimitPerR
    PVGraphReport.Sections.Item("Section2").Controls.Item("lblLineDesc2").Caption = DisplayMessage("Lower limit = ") & PVXScaleLimitPerL
    PVGraphReport.Sections.Item("Section2").Controls.Item("Explanation2").Caption = DisplayMessage("% deviation from expected internal diameter versus time (frame number)") 'PCN2111
    PVGraphReport.Sections.Item("Section2").Controls.Item("Explanation2").Visible = True
ElseIf PVGraphType = "Flat" Then
    'PVGraphReport.Sections.Item("Section1").Controls.Item("PVXScaleLabelC").Caption = "0"
    'PVGraphReport.Sections.Item("Section1").Controls.Item("PVXScaleLabelC2").Caption = "0"
    'Call PrecisionVisionGraph.DrawPVXLimitLines(0, 0, PVXScaleLimitPerL, PVXScaleLimitPerR)
    PVGraphReport.Sections.Item("Section2").Controls.Item("lblLineDesc1").Visible = False
    PVGraphReport.Sections.Item("Section2").Controls.Item("lblLineDesc2").Visible = False
    PVGraphReport.Sections.Item("Section2").Controls.Item("Key").Visible = False
    PVGraphReport.Sections.Item("Section2").Controls.Item("Line2").Visible = False
    PVGraphReport.Sections.Item("Section2").Controls.Item("Line3").Visible = False
'vvvv PCN2703 *****************************************
ElseIf PVGraphType = "XYDiameter" Then
    PVGraphReport.Sections.Item("Section2").Controls.Item("lblLineDesc1").Caption = DisplayMessage("Upper limit = ") & PVXScaleLimitPerR
    PVGraphReport.Sections.Item("Section2").Controls.Item("lblLineDesc2").Caption = DisplayMessage("Lower limit = ") & PVXScaleLimitPerL
'^^^^ *************************************************
End If

'ObjTopPos = ObjTopPosOffset + PrecisionVisionGraph.PVXScaleLabelL.left
