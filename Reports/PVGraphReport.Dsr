Private Sub DataReport_Initialize() 'PCNGL040103
'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
'Form_Load Sub  Geoff Logan geofflogan@cbsys.co.nz
'
'Revision history
'   V0.0    Geoff Logan,   04/01/03   Building framework
'
'Description:
'
'Purpose:
'
'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
On Error GoTo Err_Handler
Dim GraphNo As Integer 'PCNGL160103 'Used to select which form graph to draw in.
Dim GraphWidthMaxFrames As Long 'PCNGL160103
Dim GraphFrameStart As Long 'PCN3275
Dim GraphControlName As String
Dim CurrentGraphControlNo As Integer

ConvertLanguageInReport Me, Language 'PCN2111


If Printer.PaperSize <> vbPRPSA4 Then Printer.PaperSize = vbPRPSA4
Printer.Orientation = vbPRORLandscape 'PCNML190304

Call ReportMarginsSet("PVGraphReport") 'PCN3069


'Setup Report Page header information
Set PVGraphReport.Sections.Item("Section2").Controls.Item("Logo").Picture = LoadPicture(LocToSave & "LOGO.JPG") 'PCN2155
PVGraphReport.Sections.Item("Section2").Controls.Item("PrintedDate").Caption = DisplayMessage("Printed:") & " " & Date

'Naming report for PVGraph displayed
If PrecisionVisionGraph.Label_GraphName(0) <> "" Then
'    PVGraphReport.Sections.Item("Section2").Controls.Item("ReportTitle").Caption = DisplayMessage("Pipe ") & PrecisionVisionGraph.Label_GraphName & DisplayMessage(" Analysis Report") 'PCN2111
    PVGraphReport.Sections.Item("Section2").Controls.Item("ReportTitle").Caption = PrecisionVisionGraph.Label_GraphName(0) & DisplayMessage(" Analysis Report") 'PCN2111 'PCN3275
End If
    

'vvvv Testing
'vvvv PCN2126 **************************************
'GraphWidthMaxFrames = 2000 'PCNGL160103
If PVDataNoOfLines <= 4000 Then
    GraphWidthMaxFrames = 2000
ElseIf PVDataNoOfLines <= 6000 Then
    GraphWidthMaxFrames = 3000
ElseIf PVDataNoOfLines <= 8000 Then
    GraphWidthMaxFrames = 4000
ElseIf PVDataNoOfLines <= 12000 Then
    GraphWidthMaxFrames = 6000
ElseIf PVDataNoOfLines <= 24000 Then
    GraphWidthMaxFrames = 12000
Else
    GraphWidthMaxFrames = 12000
End If
'^^^^ **********************************************

'GraphWidthMaxFrames = 2000


''''If PVDataNoOfLines <= GraphWidthMaxFrames Then
''''    GraphNo = 0
''''    Call PVGraphReport_DrawGraph(GraphNo, 1, PVDataNoOfLines) 'PCNGL160103
''''    Set PVGraphReport.Sections.Item("Section1").Controls.Item("PVReportGraph1").Picture = PrecisionVisionGraph.PVGraphForReports(0).Image
''''    GraphNo = 1
''''    Call PVGraphReport_DrawGraph(GraphNo, GraphWidthMaxFrames, GraphWidthMaxFrames) 'PCNGL160103
''''Else
''''    GraphNo = 0
''''    Call PVGraphReport_DrawGraph(GraphNo, 1, GraphWidthMaxFrames) 'PCNGL160103
''''    Set PVGraphReport.Sections.Item("Section1").Controls.Item("PVReportGraph1").Picture = PrecisionVisionGraph.PVGraphForReports(0).Image
''''    GraphNo = 1
'''''    Call PVGraphReport_DrawGraph(GraphNo, GraphWidthMaxFrames, PVDataNoOfLines) 'PCNGL160103
''''    Call PVGraphReport_DrawGraph(GraphNo, GraphWidthMaxFrames, PVDataNoOfLines) 'PCNGL160103 'PCN3275
''''End If
''''Set PVGraphReport.Sections.Item("Section1").Controls.Item("PVReportGraph2").Picture = PrecisionVisionGraph.PVGraphForReports(1).Image

'Could do more graphs if required ... probably will be.
'If PVDataNoOfLines > 4000 Then
'    GraphFrameStart = 4000
'    GraphNo = 0
'    Call PVGraphReport_DrawGraph(GraphNo, GraphFrameStart, GraphFrameStart + GraphWidthMaxFrames) 'PCNGL160103
'    Set PVGraphReport.Sections.Item("Section1").Controls.Item("PVReportGraph3").Picture = PrecisionVisionGraph.PVGraphForReports(0).Image
'    GraphNo = 1
'    Call PVGraphReport_DrawGraph(GraphNo, GraphFrameStart + GraphWidthMaxFrames, PVDataNoOfLines) 'PCNGL160103
'    Set PVGraphReport.Sections.Item("Section1").Controls.Item("PVReportGraph4").Picture = PrecisionVisionGraph.PVGraphForReports(1).Image
'
'
'End If

GraphFrameStart = 1
GraphNo = 0
CurrentGraphControlNo = 1
Call FormatReportForTotalPages(Int(PVDataNoOfLines / (2 * GraphWidthMaxFrames))) 'Additional pages

While GraphFrameStart < PVDataNoOfLines
    GraphControlName = "PVReportGraph" & CurrentGraphControlNo
    Call PVGraphReport_DrawGraph(GraphNo, GraphFrameStart, (CurrentGraphControlNo * GraphWidthMaxFrames)) 'PCNGL160103
    Set PVGraphReport.Sections.Item("Section1").Controls.Item(GraphControlName).Picture = PrecisionVisionGraph.PVGraphForReports(GraphNo).Image
    GraphFrameStart = GraphFrameStart + GraphWidthMaxFrames
    CurrentGraphControlNo = CurrentGraphControlNo + 1

Wend

Call SetupThePVGraphReportXScale

'^^^ Testing end

'Add watermark to report if the product is unregistered PCNML230103
If Registered = False Then
    PVGraphReport.Sections.Item("Section1").Controls.Item("Watermark").Visible = True
    PVGraphReport.Sections.Item("Section2").Controls.Item("YourLogoGoesHere_Label").height = 825 'PCN1848
    PVGraphReport.Sections.Item("Section2").Controls.Item("Logo").Visible = False 'PCN1848
Else
    PVGraphReport.Sections.Item("Section2").Controls.Item("YourLogoGoesHere_Label").Visible = False 'PCN1848
    PVGraphReport.Sections.Item("Section2").Controls.Item("YourLogoGoesHere_Box").Visible = False 'PCN1848
End If

'vvvv PCN3275 **********************************************
'Moved details from Section1 (Details) to Section2 (Page Header)
' Setup the report pipeline details section
PVGraphReport.Sections.Item("Section2").Controls.Item("SiteID").Caption = PipelineDetails.SiteID
PVGraphReport.Sections.Item("Section2").Controls.Item("City").Caption = PipelineDetails.City
PVGraphReport.Sections.Item("Section2").Controls.Item("AssetNo").Caption = PipelineDetails.AssetNo
PVGraphReport.Sections.Item("Section2").Controls.Item("StNode").Caption = PipelineDetails.StartNodeNo
PVGraphReport.Sections.Item("Section2").Controls.Item("FinNode").Caption = PipelineDetails.FinishNodeNo
PVGraphReport.Sections.Item("Section2").Controls.Item("PipeLength").Caption = PipelineDetails.PipeLength
PVGraphReport.Sections.Item("Section2").Controls.Item("PipeDiameter").Caption = PipelineDetails.InternalDiameterExpected
PVGraphReport.Sections.Item("Section2").Controls.Item("PipeMaterial").Caption = PipelineDetails.Material
PVGraphReport.Sections.Item("Section2").Controls.Item("StLocation").Caption = PipelineDetails.StartNodeLocation 'PCN3222
PVGraphReport.Sections.Item("Section2").Controls.Item("FinLocation").Caption = PipelineDetails.FinishNodeLocation 'PCN3222
PVGraphReport.Sections.Item("Section2").Controls.Item("sDate").Caption = PipelineDetails.sDate 'PCN3222
PVGraphReport.Sections.Item("Section2").Controls.Item("Comments").Caption = PipelineInfo.Comments 'PCN3222
If ConfigInfo.Units = "mm" Then
    PVGraphReport.Sections.Item("Section2").Controls.Item("LengthUnits").Caption = "m"
    PVGraphReport.Sections.Item("Section2").Controls.Item("DiameterUnits").Caption = "mm"
    If Trim(ConfigInfo.DistanceProcessMethod) <> "None" And ConfigInfo.DistanceStart <> -1 Then 'PCN2538
        PVGraphReport.Sections.Item("Section1").Controls.Item("PVGraphXScale_Units").Caption = "m" 'PCN2463
        PVGraphReport.Sections.Item("Section1").Controls.Item("PVGraphXScale2_Units").Caption = "m" 'PCN2463
    End If
Else
    PVGraphReport.Sections.Item("Section2").Controls.Item("LengthUnits").Caption = "ft"
    PVGraphReport.Sections.Item("Section2").Controls.Item("DiameterUnits").Caption = "in"
    If Trim(ConfigInfo.DistanceProcessMethod) <> "None" And ConfigInfo.DistanceStart <> -1 Then 'PCN2538
        PVGraphReport.Sections.Item("Section1").Controls.Item("PVGraphXScale_Units").Caption = "ft" 'PCN2463
        PVGraphReport.Sections.Item("Section1").Controls.Item("PVGraphXScale2_Units").Caption = "ft" 'PCN2463
    End If
End If
 
'vvvv PCN3067 ****************************
If PVGraphType = "Flat" Then
    'If a Flat3D report then set up the key
    Dim ReportSection As Section
    Set ReportSection = PVGraphReport.Sections.Item("Section2") 'PCN3275
    Call Flat3DKeySetup(ReportSection)
End If
'^^^^ ************************************

' Setup the report footer details
Set PVGraphReport.Sections.Item("Section3").Controls.Item("ClearLineLogo").Picture = LoadResPicture(101, vbResBitmap)
PVGraphReport.Sections.Item("Section3").Controls.Item("CompanyName_lbl").Caption = CompanyName
PVGraphReport.Sections.Item("Section3").Controls.Item("Phone_lbl").Caption = PhoneNo
'PVGraphReport.Sections.Item("Section3").Controls.Item("PageNo_lbl").Caption = "1/1" 'PCN3275

Orientation = rptOrientLandscape
LeftMargin = 1000

'vvvv PCN2218 *******************
Dim DE As DataEnvironment1
Set DE = DataEnvironment1

Set Me.DataSource = DE
'^^^^ ***************************

Exit Sub
Err_Handler:
Select Case Err
    Case 53 'file not found
        Resume Next
    Case Else
        MsgBox Err & " - " & error$
        Resume Next
End Select
End Sub

