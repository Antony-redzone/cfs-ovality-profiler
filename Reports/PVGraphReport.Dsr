    PVGraphReport.Sections.Item("Section2").Controls.Item("Explanation2").Caption = DisplayMessage("*q per ASTM F 1216 Standard Practice for Rehabilitation of Existing Pipelines and Conduits by the Inversion and Curing of a Resin-Impregnated Tube.") 'PCN2111
