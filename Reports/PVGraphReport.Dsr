    PVGraphReport.Sections.Item("Section2").Controls.Item("lblLineDesc1").Caption = DisplayMessage("Upper limit = ") & PVXScaleLimitPerR
    PVGraphReport.Sections.Item("Section2").Controls.Item("lblLineDesc2").Caption = DisplayMessage("Lower limit = ") & PVXScaleLimitPerL
ElseIf PVGraphType = "Capacity" Then
'    PVGraphReport.Sections.Item("Section1").Controls.Item("PVXScaleLabelC").Caption = "0"
'    PVGraphReport.Sections.Item("Section1").Controls.Item("PVXScaleLabelC2").Caption = "0"
    Call PrecisionVisionGraph.RepositionPVXLimitMarkers
    PVGraphReport.Sections.Item("Section2").Controls.Item("lblLineDesc1").Caption = DisplayMessage("Upper limit = ") & PVXScaleLimitPerR
    PVGraphReport.Sections.Item("Section2").Controls.Item("lblLineDesc2").Caption = DisplayMessage("Lower limit = ") & PVXScaleLimitPerL
