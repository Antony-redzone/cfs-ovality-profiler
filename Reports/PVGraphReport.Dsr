'PVGraphReport.Sections.Item("Section1").Controls.Item("PVXScaleLabelR2").Caption = PrecisionVisionGraph.PVXScaleLabelR.Caption

Exit Function
Err_Handler:
Select Case Err
    Case Else
        MsgBox Err & " - " & error$
End Select
End Function

Function FormatReportForTotalPages(TotalNoOfPages As Integer)
'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
'Name    : FormatReportForTotalPages
'Created : 4 January 2005, PCN3275
'Updated :
'Prg By  : Geoff Logan
'Param   :
'Desc    : Formats this report for the correct number of pages
'Usage   :
'~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
On Error GoTo Err_Handler
Dim SpacingBetweenGraphs As Integer
Dim SpacingBetweenPages As Long
Dim MaxNoOfReportPages As Integer
Dim GraphControlName As String
Dim CurrentGraphControlNo As Integer
Dim PageIndex As Integer
Dim GraphTop As Long

SpacingBetweenGraphs = 300
SpacingBetweenPages = 25000

MaxNoOfReportPages = 4

With PVGraphReport.Sections.Item("Section1")

.height = (TotalNoOfPages + 1) * SpacingBetweenPages

If TotalNoOfPages > MaxNoOfReportPages Then TotalNoOfPages = MaxNoOfReportPages

For PageIndex = 1 To (2 * TotalNoOfPages) Step 2
    'Format the top graph for the current page
    CurrentGraphControlNo = PageIndex + 2
    GraphControlName = "PVReportGraph" & CurrentGraphControlNo
    GraphTop = PageIndex * SpacingBetweenPages
    .Controls.Item(GraphControlName).Top = GraphTop
    .Controls.Item(GraphControlName).left = .Controls.Item("PVReportGraph1").left
    .Controls.Item(GraphControlName).width = .Controls.Item("PVReportGraph1").width
'    Set .Controls.Item(GraphControlName).Picture = PrecisionVisionGraph.PVGraphForReports(0).Image
    .Controls.Item(GraphControlName).Visible = True
    
    'Format the bottom graph for the current page
    GraphTop = GraphTop + .Controls.Item(GraphControlName).height + SpacingBetweenGraphs
    GraphControlName = "PVReportGraph" & CurrentGraphControlNo + 1
    .Controls.Item(GraphControlName).Top = GraphTop
    .Controls.Item(GraphControlName).left = .Controls.Item("PVReportGraph1").left
    .Controls.Item(GraphControlName).width = .Controls.Item("PVReportGraph1").width
'    Set .Controls.Item(GraphControlName).Picture = PrecisionVisionGraph.PVGraphForReports(0).Image
    .Controls.Item(GraphControlName).Visible = True
    
Next PageIndex

End With


Exit Function
Err_Handler:
    MsgBox Err & error$
End Function
